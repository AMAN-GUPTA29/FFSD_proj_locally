<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Contact Us</title>
    <link href="./css/contactus.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
</head>

<body>
    <%- include('navbar_customer') %>


    <div class="container my-5">
        <h1 class="m-5">Contact Us</h1>

        <div class="m-5">
            <form action="xyz.com">
                <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">Email<div class="text-danger">*</div></span>
                    <input type="text" class="form-control" placeholder="email@xyz.com" aria-label="Username"
                        aria-describedby="basic-addon1" required>
                </div>

                <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">Name</span>
                    <input type="text" class="form-control" placeholder="email@xyz.com" aria-label="Username"
                        aria-describedby="basic-addon1" required>
                </div>
                <div id="a6">
                    <label for="options">Select your issue </label>
                    <select id="o1" name="options">
                      <option value="technical">technical</option>
                      <option value="payment related">payment related</option>
                      <option value="service quality">service quality</option>
                      <option value="delayed service">delayed service</option>
                    </select>
                    </div>
                    <br>

                <div class="input-group mb-3">
                    <span class="input-group-text">Enter your  additional concerns</span>
                    <textarea class="form-control" aria-label="With textarea"></textarea>
                </div>
               
                <div>
                    <input type="submit" class="input-group btn btn-outline-success">
                </div>
            </form>
        </div>
    </div>
    <%- include('footer_customer') %>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
        crossorigin="anonymous"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
        crossorigin="anonymous"></script>
</body>

</html>