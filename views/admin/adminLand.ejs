    <style>
        h1,h2,.navbar-brand,.fs-1{
            font-family:Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
        }

        .Stastics{
            font-size: 35px;
            font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif; 
            padding: 10px;
            color: white;
            min-height: 150px;
            background-color: rgb(32, 110, 212);
            overflow: hidden;
        }
        .dailyStastics{
            font-weight:800;
            font-size: 30px;
            color: #333;
        }
        .buttons{
            margin-top: -70px;
        }
        .content{
            box-shadow: 0 0rem 1rem rgba(119, 111, 111, 0.3);
        }
        .content2{
            box-shadow: 0 0rem 1rem rgba(119, 111, 111, 0.3);
        }
        @keyframes zoom-in {
            0% {
             transform: scale(1, 1);
            }
            100% {
             transform: scale(1.02, 1.02);
            }
        }
        .content:hover,.content2:hover{
            box-shadow: 0 0rem 3rem rgba(0, 0, 0, 0.3);
            animation: zoom-in 0.6s forwards;
        }
        .link:hover{
            box-shadow: 0 0rem 3rem rgba(0, 0, 0, 0.3);
            animation: zoom-in 0.6s forwards;
        }
        .btn {
        display: inline-block;
        padding: 20px 40px;
        margin: 10px;
        border: none;
        border-radius: 5px;
        background-color: #007bff;
        color: #fff;
        font-size: 18px;
        text-align: center;
        transition: all 0.3s ease;
        cursor: pointer;
        box-shadow: 0 3px 6px rgba(0,0,0,0.1);
      }

      .btn:hover {
        background-color: #0069d9;
        box-shadow: 0 0rem 1rem rgba(200, 187, 187, 0.564);
        box-shadow: 0 0rem 3rem rgba(255, 255, 255, 0.733);
        animation: zoom-in 0.6s forwards;
      }

      .btn i {
        margin-right: 5px;
      }

      .btn-users {
        background-color: #dc3545;
      }

      .btn-traffic {
        background-color: #17a2b8;
      }

      .btn-revenue {
        background-color: #ffc107;
      }

      .btn-tools {
        background-color: #28a745;
      }

      .btn-messages {
        background-color: #007bff;
      }
      .bs-cards{
        width: fit-content;
block-size: fit-content; 
      }
      .bs-cards:hover,.manage:hover,.welcome:hover{
        animation: zoom-in 0.6s forwards;
      }
      .matter{
        font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
        font-size: 18px;
      }
      

     

    </style>
</head>
